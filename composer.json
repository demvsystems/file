{
    "name": "demv/file",
    "description": "demv file",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/demvsystems/file",
    "keywords": [],
    "authors": [
        {
            "name": "DEMV-Systems",
            "email": "packages@demv.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "dgame/php-ensurance": "^1.0 || ^2.0",
        "ralouphie/mimey": "1.* || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "codeception/specify": "^0.4.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "DEMV\\File\\": "src/",
            "DEMV\\File\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
