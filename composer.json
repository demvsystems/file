{
    "name": "demv/file",
    "description": "demv file",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/demvsystems/file",
    "keywords": [],
    "authors": [
        {
            "name": "DEMV-Systems",
            "email": "packages@demv.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "dgame/php-ensurance": "^2.0",
        "ralouphie/mimey": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "codeception/specify": "^0.4.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "DEMV\\File\\": "src/",
            "DEMV\\File\\Test\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.33"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:demvsystems/mimey.git",
            "no-api": true
        }
    ],
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
